{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": ["es2020"],
    "sourceMap": false,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "resolveJsonModule": true,
  }
}
